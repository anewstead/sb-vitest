{
  "name": "sb-vitest",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "PLAYWRIGHT_BROWSERS_PATH=0 playwright install chromium --with-deps --no-shell",
    "dev": "vite --host --open --port 3000",
    "dev-sb": "storybook dev",
    "build": "vite build",
    "build-sb": "pnpm run build-storybook",
    "build-storybook": "storybook build",
    "build-stats": "analyze",
    "start": "vite preview --host",
    "start-sb": "http-server storybook-static",
    "lint": "pnpm run lint-js; pnpm run lint-style; pnpm run pretty",
    "lint-js": "eslint . --fix",
    "lint-style": "stylelint ./'**/*.{css,scss}' --fix --cache --cache-location=node_modules/.cache/stylelint/",
    "pretty": "prettier . --write --log-level warn --cache --no-error-on-unmatched-pattern  --ignore-unknown",
    "test": "vitest --coverage",
    "test-fn": "vitest --project=fn",
    "test-sb": "vitest --project=sb",
    "test-node": "vitest --project=node"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,cjs,mjs,json}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss}": [
      "stylelint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/material-icons": "^5.2.5",
    "@fontsource/roboto": "^5.2.5",
    "@hookform/resolvers": "^5.0.1",
    "@mui/icons-material": "^7.1.0",
    "@mui/material": "^7.1.0",
    "@mui/utils": "^7.1.0",
    "@reduxjs/toolkit": "^2.8.2",
    "@tailwindcss/vite": "^4.1.7",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.0",
    "axios": "^1.9.0",
    "axios-hooks": "^5.1.1",
    "clsx": "^2.1.1",
    "dompurify": "^3.2.5",
    "html-react-parser": "^5.2.5",
    "i18next": "^25.1.3",
    "i18next-browser-languagedetector": "^8.1.0",
    "i18next-http-backend": "^3.0.2",
    "jsdom": "^26.1.0",
    "marked": "^15.0.11",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.4",
    "react-i18next": "^15.5.1",
    "react-lazily": "^0.9.2",
    "react-redux": "^9.2.0",
    "react-router": "^7.6.0",
    "tailwindcss": "^4.1.7",
    "zod": "3.24.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@storybook/addon-a11y": "^8.6.14",
    "@storybook/addon-docs": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/addon-themes": "^8.6.14",
    "@storybook/addon-viewport": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/experimental-addon-test": "^8.6.14",
    "@storybook/manager-api": "^8.6.14",
    "@storybook/preview-api": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@storybook/theming": "^8.6.14",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.15.18",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/browser": "3.1.3",
    "@vitest/coverage-v8": "3.1.3",
    "@vitest/eslint-plugin": "^1.2.0",
    "@vitest/ui": "3.1.3",
    "deepmerge-ts": "^7.1.5",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-check-file": "^3.2.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-json": "^4.0.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^0.12.0",
    "globals": "^16.1.0",
    "http-server": "^14.1.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.0.0",
    "msw": "^2.7.6",
    "msw-storybook-addon": "^2.0.4",
    "playwright": "^1.52.0",
    "prettier": "^3.5.3",
    "prettier-plugin-jsdoc": "^1.3.2",
    "prettier-plugin-sort-json": "^4.1.1",
    "storybook": "^8.6.14",
    "storybook-addon-remix-react-router": "^4.0.1",
    "storybook-dark-mode": "^4.0.2",
    "storybook-react-i18next": "^3.2.1",
    "stylelint": "^16.19.1",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-config-standard-scss": "^15.0.0",
    "stylelint-scss": "^6.12.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.32.1",
    "typescript-plugin-css-modules": "^5.1.0",
    "vite": "^6.3.5",
    "vite-bundle-analyzer": "^0.20.2",
    "vite-plugin-checker": "^0.9.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "3.1.3",
    "vitest-browser-react": "^0.1.1"
  },
  "msw": {
    "workerDirectory": [
      ".storybook/static"
    ]
  }
}
