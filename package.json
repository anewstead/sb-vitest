{
  "name": "sb-vitest",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "preinstall": "npx only-allow pnpm",
    "dev": "vite --host --open --port 3003",
    "dev-sb": "storybook dev --port 6006",
    "build": "tsc && vite build",
    "build-sb": "npm run build-storybook",
    "build-storybook": "storybook build --quiet --loglevel warn",
    "build-stats": "analyze",
    "start": "vite preview --host --port 3003",
    "start-sb": "http-server storybook-static --port 6006",
    "lint": "npm run lint-js; npm run lint-style; npm run pretty",
    "lint-js": "eslint . --fix",
    "lint-style": "stylelint ./'**/*.{css,scss}' --fix --cache",
    "pretty": "prettier . --write --log-level warn --cache --no-error-on-unmatched-pattern  --ignore-unknown",
    "test": "vitest --watch=false",
    "test-fn": "vitest --watch=false --project=functions",
    "test-sb": "vitest --watch=false --project=storybook"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,cjs,mjs,json}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss}": [
      "stylelint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.6",
    "@eslint/js": "^9.25.0",
    "@storybook/addon-a11y": "^8.6.12",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/blocks": "^8.6.12",
    "@storybook/experimental-addon-test": "^8.6.12",
    "@storybook/react": "^8.6.12",
    "@storybook/react-vite": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/parser": "^8.30.1",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/browser": "^3.1.1",
    "@vitest/coverage-v8": "^3.1.1",
    "@vitest/eslint-plugin": "^1.1.43",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-import-resolver-typescript": "^4.3.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-json": "^4.0.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-storybook": "^0.12.0",
    "globals": "^16.0.0",
    "http-server": "^14.1.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "msw": "^2.7.5",
    "msw-storybook-addon": "^2.0.4",
    "playwright": "^1.52.0",
    "prettier": "^3.5.3",
    "prettier-plugin-jsdoc": "^1.3.2",
    "prettier-plugin-sort-json": "^4.1.1",
    "storybook": "^8.6.12",
    "stylelint": "^16.18.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-scss": "^6.11.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "typescript-plugin-css-modules": "^5.1.0",
    "vite": "^6.3.2",
    "vite-bundle-analyzer": "^0.18.1",
    "vite-plugin-checker": "^0.9.1",
    "vitest": "^3.1.1",
    "vitest-browser-react": "^0.1.1"
  },
  "msw": {
    "workerDirectory": [
      ".storybook/static"
    ]
  }
}
